<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

<class name="es.udl.asic.sakaiproject.tool.einahelpdesk.mappings.Campus" table="CAMPUS" polymorphism="implicit">
    <id name="id_campus" column="ID_CAMPUS">
      <generator class="assigned"/>
    </id>
    <property name="nom" column="NOM" type="java.lang.String" />
    <property name="responsable" column="RESPONSABLE" type="java.lang.String"/>
    <property name="activat" column="ACTIVAT" type="java.lang.Integer"/>

  
  </class>
</hibernate-mapping>
